{
  "metadata": [
    {
      "src": [
        {
          "src": "../UXAV.AVnet.Core",

          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "assets/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "templates/custom"
    ],
    "globalMetadata": {
      "_appName": "",
      "_appTitle": "AVnet Core",
      "_appLogoPath": "assets/avnet_logo.svg",
      "_enableSearch": true,
      "pdf": false
    }
  }
}